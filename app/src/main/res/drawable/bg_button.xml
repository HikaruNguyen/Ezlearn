<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <gradient android:angle="90" android:endColor="@color/first_slide_background" android:startColor="@color/first_slide_background2" android:type="linear" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">

            <solid android:color="@color/first_slide_background" />
            <corners android:radius="5dp" />

        </shape>
    </item>
</selector>